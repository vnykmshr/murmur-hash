{
  "name": "murmur-hash",
  "version": "2.0.0",
  "description": "Fast MurmurHash3 with 128-bit support for Node.js and browsers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "node --test --import tsx test/*.test.ts",
    "bench": "tsx bench/index.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "murmur",
    "murmurhash",
    "murmurhash3",
    "hash",
    "128-bit",
    "non-cryptographic",
    "fingerprint",
    "bloom-filter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnykmshr/murmur-hash.git"
  },
  "author": "vnykmshr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnykmshr/murmur-hash/issues"
  },
  "homepage": "https://github.com/vnykmshr/murmur-hash#readme",
  "engines": {
    "node": ">=20.0.0"
  }
}
